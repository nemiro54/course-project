@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
 
<h2 localize-content>List of Users</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-outline-primary" asp-action="Edit" asp-route-userid="@user.Id" localize-content>Access Rights</a>
            </td>
        </tr>
    }
</table>